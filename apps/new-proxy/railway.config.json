{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "./apps/new-proxy/Dockerfile",
    "watchPatterns": [
        "apps/new-proxy/**/*",
        "turbo.json",
        "package.json",
        "bun.lock"
    ]
  },
  "deploy": {
    "healthcheckPath": "/health",
    "healthcheckTimeout": 100,
    "restartPolicyType": "never"
  }
}
