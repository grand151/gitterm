{
  "name": "proxy",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile proxy",
    "dev": "bun run --hot src/index.ts",
    "dev:ngrok": "bun run --hot src/index.ts && ngrok http 4000",
    "start": "bun run dist/index.mjs"
  },
  "dependencies": {
    "@gitpad/auth": "workspace:*",
    "@gitpad/db": "workspace:*",
    "dotenv": "catalog:",
    "hono": "catalog:",
    "http-proxy": "^1.18.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@gitpad/config": "workspace:*",
    "@types/bun": "^1.2.6",
    "@types/ws": "^8.18.1",
    "tsdown": "^0.16.5",
    "typescript": "catalog:"
  }
}
