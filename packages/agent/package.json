{
  "name": "@opeoginni/gitterm-agent",
  "version": "0.1.13",
  "description": "Tunnel agent for exposing local development ports through gitterm.dev",
  "keywords": [
    "development",
    "gitterm",
    "localtunnel",
    "ngrok",
    "opencode",
    "proxy",
    "tunnel"
  ],
  "homepage": "https://gitterm.dev",
  "license": "MIT",
  "author": "Opeyemi Oginni",
  "bin": {
    "gitterm-agent": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:local": "bun --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "prepublishOnly": "bun run build",
    "check-types": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@types/yargs": "^17.0.35",
    "chalk": "^5.6.2",
    "yargs": "^18.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
