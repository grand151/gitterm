{
  "name": "@gitterm/api",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./context": {
      "default": "./src/context.ts"
    },
    "./config": {
      "default": "./src/config/index.ts"
    },
    "./config/*": {
      "default": "./src/config/*.ts"
    },
    "./routers/*": {
      "default": "./src/routers/*.ts"
    },
    "./providers": {
      "default": "./src/providers/index.ts"
    },
    "./providers/*": {
      "default": "./src/providers/*.ts"
    },
    "./utils/*": {
      "default": "./src/utils/*.ts"
    },
    "./events/*": {
      "default": "./src/events/*.ts"
    },
    "./service/*": {
      "default": "./src/service/*.ts"
    },
    "./client": {
      "default": "./src/client/index.ts"
    },
    "./client/internal": {
      "default": "./src/client/internal.ts"
    }
  },
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts",
    "wrangler:deploy": "wrangler deploy",
    "wrangler:dev": "wrangler dev",
    "wrangler:start": "wrangler dev",
    "wrangler:cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.938.0",
    "@aws-sdk/client-sts": "^3.936.0",
    "@cloudflare/sandbox": "^0.6.10",
    "@cloudflare/workers-types": "^4.20260107.1",
    "@daytonaio/sdk": "^0.129.0",
    "@gitterm/auth": "workspace:*",
    "@gitterm/db": "workspace:*",
    "@gitterm/env": "workspace:*",
    "@gitterm/redis": "workspace:*",
    "@gitterm/schema": "workspace:*",
    "@octokit/auth-app": "^7.1.3",
    "@octokit/rest": "^21.0.2",
    "@octokit/webhooks": "^14.2.0",
    "@opencode-ai/plugin": "^1.1.8",
    "@opencode-ai/sdk": "^1.1.4",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "cloudflare": "^5.2.0",
    "discord.js": "^14.25.1",
    "graphql": "^16.12.0",
    "hono": "catalog:",
    "jsonwebtoken": "^9.0.2",
    "wrangler": "^4.58.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@gitterm/config": "workspace:*",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
