{
  "name": "@gitpad/api",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./context": {
      "default": "./src/context.ts"
    },
    "./routers/*": {
      "default": "./src/routers/*.ts"
    },
    "./providers": {
      "default": "./src/providers/index.ts"
    },
    "./providers/*": {
      "default": "./src/providers/*.ts"
    },
    "./utils/*": {
      "default": "./src/utils/*.ts"
    },
    "./events/*": {
      "default": "./src/events/*.ts"
    },
    "./service/*": {
      "default": "./src/service/*.ts"
    },
    "./client": {
      "default": "./src/client/index.ts"
    },
    "./client/internal": {
      "default": "./src/client/internal.ts"
    }
  },
  "type": "module",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "devDependencies": {
    "@gitpad/config": "workspace:*",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.938.0",
    "@aws-sdk/client-sts": "^3.936.0",
    "@gitpad/auth": "workspace:*",
    "@gitpad/db": "workspace:*",
    "@gitpad/schema": "workspace:*",
    "@gitpad/redis": "workspace:*",
    "@octokit/auth-app": "^7.1.3",
    "@octokit/rest": "^21.0.2",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "graphql": "^16.12.0",
    "hono": "catalog:",
    "jsonwebtoken": "^9.0.2",
    "zod": "catalog:"
  }
}
